{
  "name": "astro-project",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "node scripts/dev.mjs",
    "dev:all": "node scripts/dev.mjs",
    "dev:raw": "npm run ports:free && astro dev",
    "start": "astro dev",
    "build": "astro build",
    "postbuild": "node scripts/fix-runtime.js",
    "preview": "astro preview",
    "astro": "astro",
    "check-secrets": "node scripts/check-secrets.js",
    "db:init": "node scripts/db-init.mjs",
    "ports:free": "powershell -ExecutionPolicy Bypass -File scripts/ports-free.ps1",
    "smoke": "powershell -ExecutionPolicy Bypass -File scripts/smoke.ps1",
    "shots": "tsx scripts/screenshots.ts",
    "test:runtime": "node scripts/test-runtime-config.js",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:all": "playwright test",
    "test:visual": "playwright test tests/visual/",
    "test:visual:update": "playwright test tests/visual/ --update-snapshots",
    "test": "npm run test:unit && npm run test:e2e"
  },
  "dependencies": {
    "@astrojs/react": "^3.3.1",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.8.2",
    "@sendgrid/mail": "^8.1.6",
    "@vercel/analytics": "^1.6.1",
    "@vercel/postgres": "^0.10.0",
    "@vercel/speed-insights": "^1.3.1",
    "astro": "^4.0.0",
    "lucide-react": "^0.561.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitest/coverage-v8": "^4.0.18",
    "happy-dom": "^20.6.1",
    "playwright": "^1.57.0",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
}
